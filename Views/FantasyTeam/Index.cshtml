@model IEnumerable<FantasyTeam>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.QB1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QB2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RB1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RB2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WR1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WR2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Flex)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Kicker)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Defense)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SkillLevel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TouchdownsScoredLastYear)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TouchDownsProjected)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.QB1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QB2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RB1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RB2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WR1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WR2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Flex)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Kicker)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Defense)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SkillLevel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TouchdownsScoredLastYear)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TouchDownsProjected)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
